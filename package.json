{
  "name": "react-swap",
  "version": "1.1.0",
  "description": "React component-wrapper to swap one element with another and back",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:lib && npm run build:example && npm run build:build",
    "build:clean": "rm -rf lib example build",
    "build:lib": "babel src --out-dir lib --source-maps --ignore src/example",
    "build:example": "webpack",
    "build:build": "BUILD=1 webpack",
    "prepublish": "npm run build",
    "start": "NODE_ENV=development webpack-dev-server",
    "lint": "eslint .",
    "test": "karma start ./karma.conf.js --single-run --reporters nyan",
    "test:dev": "karma start ./karma.conf.js --no-single-run --auto-watch --reporters nyan",
    "test:cov": "COVERAGE=1 karma start ./karma.conf.js --single-run --reporters coverage",
    "check": "npm run lint && npm run test && npm run build",
    "coveralls": "cat ./reports/coverage/lcov.info | `npm bin`/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nkbt/react-swap.git"
  },
  "keywords": [
    "component",
    "react-component",
    "react",
    "react-swap",
    "swap",
    "toggle",
    "switch",
    "hover",
    "dropdown"
  ],
  "author": "Nik Butenko <nik@butenko.me> (http://butenko.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkbt/react-swap/issues"
  },
  "homepage": "https://github.com/nkbt/react-swap",
  "peerDependencies": {
    "react": "^0.13 || ~0.14.0-beta"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.21",
    "babel-loader": "^5.3.2",
    "coveralls": "^2.11.3",
    "eslint": "1.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.2.1",
    "html-webpack-plugin": "^1.6.1",
    "inject-loader": "^2.0.1",
    "isparta-loader": "^0.2.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.8",
    "karma-coverage": "^0.5.0",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.3",
    "karma-nyan-reporter": "0.2.2",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "mkdirp": "^0.5.1",
    "node-libs-browser": "^0.5.2",
    "phantomjs": "^1.9.17",
    "react-hot-loader": "^1.2.8",
    "rimraf": "^2.4.2",
    "tap-xunit": "^1.1.1",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
}
